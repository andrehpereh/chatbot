            "\nimport kfp\nfrom kfp import dsl\nfrom kfp.dsl import *\nfrom typing import *\nfrom typing import Dict\n\ndef append_to_dictionary_op(data: dsl.Input[Dict]):\n    data['key3'] = 'new_value'  # Append an element \n    print(\"Modified Dictionary:\", data)\n\n"
